{
  "trailingComma": "none",
  "useTabs": true,
  "semi": false,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "importOrder": [
    "react",
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/providers/(.*)$",
    "^@/services/(.*)$",
    "^@/assets/(.*)$",
    "^@/types/(.*)$",
    "^@/hooks/(.*)$",
    "^../(.*)$",
    "^./(.*)$",
    "(.scss*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}